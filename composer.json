{
    "name": "ck/php-marcspec",
    "type": "library",
    "description": "PHP based MARCspec parser and validator",
    "keywords": [ "MARCspec", "MARC 21" ],
    "authors": [
        {
            "name": "Carsten Klee",
            "homepage": "http://collidoscope.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
         "phpunit/phpunit": "^5.7 | ^6",
         "ck/marcspec-test-suite": "@dev"
    },
    "autoload": {
        "psr-4": {
            "CK\\MARCspec\\": "src/",
            "CK\\MARCspec\\Exception\\": "src/Exception/"
        }
    },
    "target-dir": "CK/MARCspec"
}
